{
  "name": "adonis-api-app",
  "version": "4.1.0",
  "adonis-version": "4.1.0",
  "description": "Adonisjs boilerplate for API server with pre-configured JWT",
  "main": "index.js",
  "scripts": {
    "start": "ENV_SILENT=true node server.js",
    "test": "node ace test",
    "run:reset": "ENV_SILENT=true node ace migration:reset --force",
    "run:migration": "ENV_SILENT=true node ace migration:run --force",
    "run:rollback": "ENV_SILENT=true node ace migration:rollback --force",
    "run:seed": "ENV_SILENT=true node ace seed --force"
  },
  "keywords": [
    "adonisjs",
    "adonis-app"
  ],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "5.0.8",
    "@adonisjs/auth": "3.1.0",
    "@adonisjs/bodyparser": "2.0.9",
    "@adonisjs/cors": "1.0.7",
    "@adonisjs/drive": "1.0.4",
    "@adonisjs/fold": "4.0.9",
    "@adonisjs/framework": "5.0.13",
    "@adonisjs/ignitor": "2.0.8",
    "@adonisjs/lucid": "6.1.3",
    "@adonisjs/redis": "2.0.7",
    "@adonisjs/validator": "5.0.6",
    "@radmen/adonis-lucid-soft-deletes": "0.2.0",
    "igg-adonis-api-response": "1.2.0",
    "mysql": "2.18.1",
    "sharp": "0.25.2"
  },
  "devDependencies": {},
  "autoload": {
    "App": "./app"
  }
}
